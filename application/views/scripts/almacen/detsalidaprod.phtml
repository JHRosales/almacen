        function Guardar() {

            var detallesalida = {};
            var series = [];
            var tecnicos = [];


            //Datos generales del salida de series
            detallesalida.idsalidaprod = $("#idsalidaprod").val();
            detallesalida.obra = $("#txtobra").val();
            detallesalida.lugar = $("#txtlugar").val();
            detallesalida.fecha = $("#fecsalida").val();
            detallesalida.idtecnico = $("#txtidtecnico").val();
            detallesalida.idcliente = $("#codigoCliente").val();
            detallesalida.idcotizacion = $("#idCotizacion").val();
            detallesalida.obs = $("#txtObs").val();

            // if ($("#txtidtecnico").val() == "") {
            //     openDialogWarning("Seleccione un tecnico encargado.");
            //     return;
            // }
            if ($("#fecsalida").val() == "") {
                openDialogWarning("Seleccione la fecha.");
                return;
            }
            //Datos series
            $('#tableSeries > tbody  > tr').each(function() {
                var detseries = {};
                if ($(this).find("[name=txtCodigoSerie]").val() != '') {
                    detseries.idDetSalidaProd = $(this).find("[name=txtidDetSalidaProd]").val();
                    detseries.idSerie = $(this).find("[name=txtCodigoSerie]").val();
                    detseries.vSerie = $(this).find("[name=txtSerie]").val();
                    detseries.stock = $(this).find("[name=txtStock]").val();
                    detseries.fecsalidaProducto = $(this).find("[name=fecsalidaProducto]").val();
                    series.push(detseries);
                }
            });
            detallesalida.series = series;

            //Datos Tecnicos
            $('#tableTecnicos > tbody  > tr').each(function() {
                var detTecnicos = {};
                if ($(this).find("[name=txtIdTecnico]").val() != '') {
                    detTecnicos.idDetTecnicoProd = $(this).find("[name=txtidDetTecnicoProd]").val();
                    detTecnicos.idTecnico = $(this).find("[name=txtIdTecnico]").val();
                    detTecnicos.NombreTecnico = $(this).find("[name=txtTecnico]").val();
                    tecnicos.push(detTecnicos);
                }
            });
            detallesalida.tecnicos = tecnicos;

            var pSalidaProd = {};
            pSalidaProd.objSalidaProd = JSON.stringify(detallesalida);
            //console.log(JSON.stringify(pPapeleta));
            //console.log(papeleta);


