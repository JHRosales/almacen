                    // $("#iframeUploadDocPapeleta").attr("src", path + "papeleta/uploadarchivo?mpapeleta=" + data.idsigma);
                    //  dialogUpload.dialog("open");
                    window.open(path + "almacen/salidaprod", "_self");
                }

            }, 'json');


        };
